    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <COMReference Include="Microsoft.Office.Interop.Publisher">
      <WrapperTool>tlbimp</WrapperTool>
      <VersionMinor>3</VersionMinor>
